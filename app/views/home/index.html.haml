= form_for :search, url: search_gifs_path, method: :post do |form|
  = form.search_field :q, id: 'search_gifs'
  = form.submit 'Search'

- if @gifs
  %p= "Gifs with tag(s): #{@tags}"
  %ul.gif-list
    - @gifs.each do |gif|
      %li{ id: "#{gif["id"]}" }
        .gif-content
          %span= gif["id"]
        .gif-save-option
          = button_to "Save Gif", action: :create, params: { gif: {Â gif_api_id: gif["id"], tags: @tags } }